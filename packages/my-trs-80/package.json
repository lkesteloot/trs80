{
  "name": "my-trs-80",
  "version": "2.3.1",
  "private": true,
  "description": "TRS-80 emulator website",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "vite",
    "updateBuildInfo": "../../printBuildInfo > src/build.ts",
    "build": "npm run updateBuildInfo && tsc --noEmit && vite build",
    "clean": "rm -rf build",
    "publish": "npm run build && rsync -aP build/ lk@hitch.headcode.com:my-trs-80-website/",
    "publish-stage": "npm run build && rsync -aP build/ lk@hitch.headcode.com:my-trs-80-website/stage/"
  },
  "author": "Lawrence Kesteloot <lk@teamten.com> (https://www.teamten.com/lawrence/)",
  "repository": "https://github.com/lkesteloot/trs80.git",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "pbjs": "^0.0.14"
  },
  "dependencies": {
    "base64-js": "^1.5.1",
    "firebase": "^10.14.1",
    "firebaseui": "^6.1.0",
    "jszip": "^3.5.0",
    "lodash": "^4.17.20",
    "retrostore-api": "^1.0.0",
    "strongly-typed-events": "^3.0.0",
    "teamten-ts-utils": "^1.0.3",
    "trs80-base": "^2.3.1",
    "trs80-cassette": "^2.3.1",
    "trs80-emulator": "^2.3.1",
    "trs80-emulator-web": "^2.3.1",
    "z80-base": "^2.0.1",
    "z80-disasm": "^2.3.0",
    "z80-emulator": "^2.3.0"
  }
}
