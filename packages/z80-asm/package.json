{
  "name": "z80-asm",
  "version": "2.3.0",
  "description": "Z80 assembler in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:test": "tsc -p tsconfig.test.json",
    "generate": "tsc --project opcodes && node opcodes/dist/opcodes/GenerateJson.js",
    "docs": "typedoc -p . --target es6 --excludeNotExported --excludePrivate --out docs src && touch docs/.nojekyll",
    "test": "npm run build:test && mocha -r ts-node/register src/**/*.spec.ts",
    "clean": "rm -rf dist dist-tests",
    "prepublishOnly": "npm run build"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "z80",
    "assembler"
  ],
  "author": "Lawrence Kesteloot <lk@teamten.com> (https://www.teamten.com/lawrence/)",
  "repository": "https://github.com/lkesteloot/trs80.git",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.22",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "typedoc": "^0.15.6",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "z80-base": "^2.0.1"
  }
}
